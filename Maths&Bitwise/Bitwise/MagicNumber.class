class Main {
    public static void main(String[] args) {
        int n=6;
        System.out.println(magicNumber(n));
    }
    static int magicNumber(int n){
        int ans = 0;
        int base = 5;
        while(n>0){
            int lbit = n&1;
            n=n>>1;
            ans+=lbit*base;
            base*=5;
        }
        return ans;
    }
}