
class Main {
    public static void main(String[] args) {
        int n=40;
        int p=3;
        System.out.print(sqrt(n, p));
    }
    static double sqrt(int n, int pre){
        int s=0;
        int e=n;
        double ans=0;
        while(s<=e){
            int mid=s+(e-s)/2;
            if(mid*mid==n){
                ans=mid;
                break;
            }
            else if(mid*mid>n){
                e=mid-1;
            }
            else{
                s=mid+1;
            }
        }
        if(ans==0){
            ans=e;
        }
        double p=0.1;
        for(int i=1; i<=pre; i++){
            while(ans*ans<=n){
                ans+=p;
            }
            ans-=p;
            p/=10;
        }
        return ans;
        
    }
    
}